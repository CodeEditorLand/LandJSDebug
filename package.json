		"glob-stream": "^8.0.0",
		"got": "^11.8.6",
		"inversify": "^6.0.2",
		"js-xxhash": "^3.0.1",
		"jsonc-parser": "^3.2.1",
		"linkifyjs": "^4.1.3",
		"micromatch": "^4.0.5",
		"path-browserify": "^1.0.1",
		"picomatch": "connor4312/picomatch#2fbe90b12eafa7dde816ff8c16be9e77271b0e0b",
		"preact": "^10.19.3",
		"reflect-metadata": "^0.2.1",
