{
	"name": "js-debug",
	"displayName": "JavaScript Debugger",
	"description": "An extension for debugging Node.js programs and Chrome.",
	"main": "./src/extension.js",
	"scripts": {
		"compile": "gulp",
		"generateapis": "tsx src/build/generateDap.ts && tsx src/build/generateCdp.ts",
		"precommit": "npm-run-all --parallel test:lint test:types",
		"prepare": "husky install",
		"publish": "gulp publish",
		"test:golden": "node ./src/test/runTest.js",
		"test:types": "tsc --noEmit",
		"test:unit": "tsx node_modules/mocha/bin/mocha.js --config .mocharc.unit.js",
		"updatenodeapi": "python src/build/getNodePdl.py && prettier --write src/build/nodeCustom.ts",
		"updatetypes": "cd src/typings && npx -y @vscode/dts dev && npx -y @vscode/dts master"
	},
	"dependencies": {
		"@jridgewell/gen-mapping": "0.3.6-beta.4",
		"@jridgewell/trace-mapping": "0.3.25",
		"acorn": "8.12.1",
		"acorn-loose": "8.4.0",
		"astring": "1.8.6",
		"color": "4.2.3",
		"data-uri-to-buffer": "6.0.2",
		"default-browser": "5.2.1",
		"dotenv": "16.4.5",
		"execa": "9.3.1",
		"glob-stream": "8.0.2",
		"got": "14.4.2",
		"inversify": "6.0.2",
		"js-xxhash": "4.0.0",
		"jsonc-parser": "3.3.1",
		"linkifyjs": "4.1.3",
		"micromatch": "4.0.7",
		"path-browserify": "1.0.1",
		"picomatch": "connor4312/picomatch#2fbe90b12eafa7dde816ff8c16be9e77271b0e0b",
		"reflect-metadata": "0.2.2",
		"signale": "1.4.0",
		"source-map-support": "0.5.21",
		"to-absolute-glob": "3.0.0",
		"ws": "8.18.0"
	},
	"devDependencies": {
		"@c4312/matcha": "1.3.1",
		"@types/chai": "4.3.17",
		"@types/chai-as-promised": "7.1.8",
		"@types/chai-string": "1.4.5",
		"@types/chai-subset": "1.3.5",
		"@types/color": "3.0.6",
		"@types/debug": "4.1.12",
		"@types/diff": "5.2.1",
		"@types/estree": "1.0.5",
		"@types/glob-stream": "8.0.2",
		"@types/js-beautify": "1.14.3",
		"@types/json-schema": "7.0.15",
		"@types/linkifyjs": "2.1.7",
		"@types/long": "5.0.0",
		"@types/marked": "6.0.0",
		"@types/micromatch": "4.0.9",
		"@types/minimist": "1.2.5",
		"@types/mkdirp": "2.0.0",
		"@types/node": "22.4.0",
		"@types/signale": "1.4.7",
		"@types/sinon": "17.0.3",
		"@types/stream-buffers": "3.0.7",
		"@types/tmp": "0.2.6",
		"@types/to-absolute-glob": "2.0.3",
		"@types/ws": "8.5.12",
		"chai": "5.1.1",
		"chai-as-promised": "8.0.0",
		"chai-string": "1.5.0",
		"chai-subset": "1.6.0",
		"diff": "5.2.0",
		"esbuild": "0.23.1",
		"glob": "11.0.0",
		"https-proxy-agent": "7.0.5",
		"jszip": "3.10.1",
		"marked": "14.0.0",
		"merge2": "1.4.1",
		"minimist": "1.2.8",
		"npm-run-all": "4.1.5",
		"nyc": "17.0.0",
		"sinon": "18.0.0",
		"stream-buffers": "3.0.3",
		"tsx": "4.17.0"
	},
	"icon": "resources/logo.png",
	"enabledApiProposals": [
		"portsAttributes",
		"workspaceTrust",
		"tunnels"
	],
	"extensionKind": [
		"workspace"
	]
}
